<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PinIndicator">
        <attr name="pinLength" format="integer" />
        <attr name="radius" format="dimension" />
        <attr name="gap" format="dimension" />
        <attr name="colorDefault" format="color" />
        <attr name="colorHighlight" format="color" />
    </declare-styleable>

    <declare-styleable name="AccountChooser">
        <attr name="indicatorEnable" format="boolean" />
        <attr name="cardSize">
            <enum name="small" value="0" />
            <enum name="medium" value="1" />
            <enum name="large" value="2" />
        </attr>
    </declare-styleable>
</resources>